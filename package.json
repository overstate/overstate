    "ts-jest": "22.4.1",
    "typescript": "2.6.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "!benchmarks/**",
      "!debug/**",
      "!node_modules/**",
      "!**/*.d.ts",
      "!**/*.test.ts",
      "!index.ts",
      "**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "(/__tests__/.*|\\.test)\\.(ts|js)$",
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
