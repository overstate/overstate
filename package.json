{
  "name": "derpy",
  "version": "0.3.5",
  "license": "MIT",
  "main": "index.min.js",
  "module": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc && rollup -c",
    "build:benchmarks": "tsc -p benchmarks",
    "start": "jest --watchAll",
    "test": "jest"
  },
  "devDependencies": {
    "@types/benchmark": "1.0.31",
    "@types/jest": "21.1.8",
    "@types/node": "8.5.1",
    "benchmark": "2.1.4",
    "jest": "21.2.1",
    "rollup": "0.52.2",
    "rollup-plugin-uglify": "2.0.1",
    "ts-jest": "21.2.4",
    "typescript": "2.6.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "!node_modules/**",
      "!*.d.ts",
      "!*.test.ts",
      "!*.config.ts",
      "*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "(/__tests__/.*|\\.test)\\.(ts|js)$",
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
